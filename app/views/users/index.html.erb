<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>

    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
          <td><%=user.name%></td>
          <td><%=user.email%></td>
          <td><%=user.phone%></td>
          <td><%=user.subscribe%></td>
          <td><%=user.gender%></td>
          <td><%=user.adress%></td>
          <td><%=user.citycode%></td>
          <td><%=user.city%></td>
     </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
